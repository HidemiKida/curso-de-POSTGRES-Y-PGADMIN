create table deportes(
codigo serial,
nombre varchar(30),
profesor varchar(30),
primary key(codigo)
);

INSERT INTO deportes (nombre, profesor) VALUES ('Fútbol', 'Juan Pérez');
INSERT INTO deportes (nombre, profesor) VALUES ('Básquetbol', 'María González');
INSERT INTO deportes (nombre, profesor) VALUES ('Tenis', 'Carlos Sánchez');
INSERT INTO deportes (nombre, profesor) VALUES ('Natación', 'Ana Martínez');

create table inscriptos(
documento char(8),
codigodeporte smallint not null,
matricula char(1) --'s'=paga 'n'=impaga
);

INSERT INTO inscriptos (documento, codigodeporte, matricula) VALUES ('12345678', 2, 's');
INSERT INTO inscriptos (documento, codigodeporte, matricula) VALUES ('87654321', 2, 'n');
