create table clientes (
codigo serial, 
nombre varchar(30), 
domicilio varchar(30), 
primary key (codigo)
);
create table empleados(
documento char(8)not null, 
nombre varchar(20), 
apellido varchar (20), 
domicilio varchar(30), 
primary key (documento)
);

